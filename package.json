{
  "name": "react-pwa-boilerplate",
  "version": "0.1.0",
  "description": "A universal, reactiflux project boilerplate for a progressive web application",
  "main": "./start.js",
  "scripts": {
    "build": "eslint . && gulp build",
    "ccss": "gulp ccss",
    "cover": "istanbul cover --dir ${COVERAGE_DIR:-reports/coverage} -- _mocha tests/unit/* --recursive --compilers js:babel-register --reporter spec",
    "dev": "eslint . && gulp dev",
    "debug": "eslint . && gulp debug",
    "dumpconfig": "gulp dumpconfig",
    "fixtures:dev": "gulp fixturesDev",
    "fixtures:prod": "gulp fixturesProd",
    "functest": "node tests/functional/run-parallel '--reporter spec tests/functional/main.js'",
    "lint": "eslint .",
    "perf": "eslint . && gulp perf",
    "prod": "eslint . && gulp prod",
    "start": "node -e 'process.env.NODE_ENV = process.env.NODE_ENV || \"production\"; ./start.js;'",
    "test": "mocha tests/unit --recursive  --compilers js:babel-register --reporter spec",
    "testdebug": "mocha tests/unit --debug-brk --recursive --compilers js:babel-register --reporter spec"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "async-lock": "^0.3.8",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "cannibalizr": "localnerve/cannibalizr",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "colors": "^1.1.2",
    "del": "^2.2.0",
    "eslint": "^2.7.0",
    "fg-loadcss": "^1.2.0",
    "fontfaceobserver": "^1.5.4",
    "foundation-apps": "localnerve/foundation-apps",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-postcss": "^6.1.0",
    "gulp-sass": "^2.2.0",
    "gulp-svg2png": "^1.0.0",
    "gulp-svgmin": "^1.2.2",
    "istanbul": "^0.4.3",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mockery": "^1.6.2",
    "node-sass-asset-functions": "0.0.9",
    "precommit-hook": "^3.0.0",
    "q": "^1.4.1",
    "sw-precache": "^3.1.1",
    "wd": "^0.4.0",
    "webpack": "^1.12.15"
  },
  "dependencies": {
    "amqplib": "^0.4.1",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "connect-modrewrite": "^0.9.0",
    "cookie-parser": "^1.4.1",
    "csurf": "^1.8.3",
    "debug": "^2.2.0",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "express-error-handler": "localnerve/express-error-handler",
    "fluxible": "^1.1.0",
    "fluxible-addons-react": "^0.2.8",
    "fluxible-plugin-fetchr": "^0.3.9",
    "fluxible-router": "^0.4.13",
    "inherits": "^2.0.1",
    "lodash": "^4.11.1",
    "morgan": "^1.7.0",
    "nconf": "^0.8.4",
    "nodemailer": "^2.3.2",
    "object-assign": "^4.0.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-spinner": "localnerve/react-spinner",
    "remarkable": "^1.6.2",
    "serialize-javascript": "^1.2.0",
    "serve-favicon": "^2.3.0",
    "sitemap-xml": "localnerve/sitemap-xml",
    "superagent": "^1.8.3",
    "sw-toolbox": "^3.1.1",
    "web-push": "0.0.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localnerve/react-pwa-boilerplate.git"
  },
  "keywords": [
    "pwa",
    "react",
    "fluxible",
    "service-worker",
    "node-sass",
    "webpack",
    "node"
  ],
  "author": "Alex Grant <alex@localnerve.com> (@localnerve)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/localnerve/react-pwa-boilerplate/issues"
  },
  "homepage": "https://github.com/localnerve/react-pwa-boilerplate#readme",
  "devEngines": {
    "node": "4.x",
    "npm": "2.x"
  }
}
